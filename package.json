{
  "private": true,
  "name": "babel-proxy-server",
  "version": "1.0.0",
  "description": "Proxy that converts js files with babel",
  "main": "src/server.js",
  "author": "Daniel Kostro <kostro.d@gmail.com>",
  "scripts": {
    "server": "node src/server.js",
    "start": "npm run server",
    "proxy": "node src/server.js --noBabel",
    "server-dev": "node --watch src/server.js",
    "ngrok": "ngrok http 9898"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-amd-if-required": "^1.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5"
  },
  "license": "MIT",
  "devDependencies": {
    "ngrok": "^4.3.3"
  },
  "volta": {
    "node": "24.12.0"
  }
}
